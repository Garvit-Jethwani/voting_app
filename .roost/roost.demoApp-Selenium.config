app_dependencies:
  type: None
  content: ''
app_deploy:
  type: text
  content: |
    #/bin/sh

    HELM_OPTS="--set clusterIP=${ROOST_CLUSTER_IP},tag=${ROOST_NAMESPACE},namespace=${ROOST_NAMESPACE}"


    helm upgrade test-helm-vote helm-vote -n $ROOST_NAMESPACE --create-namespace --install $HELM_OPTS
app_test:
  - name: ballot
    configType: artillery
    configPath: ballot/test/test.yaml
  - name: voter
    configType: selenium/test
    configPath: RoostTest/voter/voter.spec.js
  - name: election-commission
    configType: selenium/test
    configPath: RoostTest/election-commission/election-commission.spec.js
app_uninstall:
  type: None
  content: ''
app_docker_compose: ''
